 // æ·»åŠ åœ°åŒºåˆ†ç»„ï¼ˆæ”¯æŒ proxies & proxy-providersï¼‰
function addRegions(config) {

  // -------------------------------
  // æƒ…å†µ 1ï¼šæ²¡æœ‰ proxiesï¼ˆåªæœ‰ proxy-providersï¼‰
  // âœ åœ°åŒº = url-testï¼ˆè‡ªåŠ¨æµ‹é€Ÿï¼‰
  // -------------------------------
  if (!config.proxies) {
    if (!config["proxy-providers"]) return;

    const providers = Object.keys(config["proxy-providers"]);
    if (providers.length === 0) return;

    let regions = [];
    let exclude = "";

    for (const region of regionConfig) {
      if (!region.name) continue;

      if (region.matcher) {
        exclude += (exclude === "" ? region.matcher : `|${region.matcher}`);

        config["proxy-groups"].push({
          ...groupBaseOption,
          name: region.name,
          type: "url-test",
          interval: test_interval,
          tolerance: test_tolerance,
          use: providers,
          filter: region.matcher,
          icon: region.icon,
        });
      } else {
        config["proxy-groups"].push({
          ...groupBaseOption,
          name: region.name,
          type: "url-test",
          use: providers,
          interval: test_interval,
          tolerance: test_tolerance,
          "exclude-filter": exclude,
          icon: region.icon,
        });
      }

      regions.push(region.name);
    }

    if (regions.length === 0) return;

    const entries = config["proxy-groups"];

    for (const entry of entries) {
      if (!entry || !entry.proxies) continue;

      if (entry.name === "èŠ‚ç‚¹é€‰æ‹©") {
        if (entry.proxies.length > 1) {
          entry.proxies.splice(2, 0, "åœ°åŒºé€‰æ‹©");
        }
      } else if (entry.name === "å…¨å±€ç›´è¿") {
        entry.proxies.push("åœ°åŒºé€‰æ‹©");
      } else if (entry.type === "select" && !entry.hasOwnProperty("include-all")) {
        entry.proxies.push(...regions);
      }
    }

    entries.splice(1, 0, {
      ...groupBaseOption,
      name: "åœ°åŒºé€‰æ‹©",
      type: "select",
      proxies: regions,
      icon: "https://fastly.jsdelivr.net/gh/Koolson/Qure@master/IconSet/Color/World_Map.png",
    });

    config["proxy-groups"] = entries;
    return;
  }

  // -------------------------------
  // æƒ…å†µ 2ï¼šè®¢é˜…é‡Œæ˜¯ proxiesï¼ˆä½ çš„æƒ…å†µï¼‰
  // âœ åœ°åŒº = çº¯æ‰‹åŠ¨ selectï¼ˆä¸ä¼šè‡ªåŠ¨æµ‹é€Ÿï¼‰
  // -------------------------------
  let names = config.proxies.map(p => p.name).filter(Boolean);
  if (names.length === 0) return;

  let regions = [];

  for (const region of regionConfig) {
    let regionNodes = [];

    if (!region.matcher) {
      regionNodes = [...names];
    } else {
      const parts = region.matcher.split("|");
      regionNodes = names.filter(name =>
        parts.some(m => name.includes(m))
      );
    }

    if (regionNodes.length === 0) continue;

    config["proxy-groups"].push({
      ...groupBaseOption,
      name: region.name,
      type: "select",
      proxies: regionNodes,
      icon: region.icon,
    });

    regions.push(region.name);
  }

  if (regions.length === 0) return;

  config["proxy-groups"].splice(1, 0, {
    ...groupBaseOption,
    name: "åœ°åŒºé€‰æ‹©",
    type: "select",
    proxies: regions,
    icon: "https://fastly.jsdelivr.net/gh/Koolson/Qure@master/IconSet/Color/World_Map.png",
  });

  for (const entry of config["proxy-groups"]) {
    if (!entry || !entry.proxies) continue;

    if (entry.name === "èŠ‚ç‚¹é€‰æ‹©") {
      entry.proxies.splice(2, 0, "åœ°åŒºé€‰æ‹©");
    } else if (entry.type === "select" && !entry["include-all"]) {
    if (!regions.includes(entry.name) && entry.name !== "åœ°åŒºé€‰æ‹©") {
        entry.proxies.push(...regions);
    }
   }
  }
}
